{"code":"import * as tslib_1 from \"tslib\";\r\nimport { Component, Input, EventEmitter, Output } from '@angular/core';\r\nvar PaginationComponent = (function () {\r\n    function PaginationComponent() {\r\n        this.goTo = new EventEmitter();\r\n        this.pages = [];\r\n    }\r\n    PaginationComponent.prototype.ngOnInit = function () {\r\n        this.active = 1;\r\n        var numPages = Math.ceil(this.count / 10);\r\n        for (var x = 0; x < numPages; x++) {\r\n            this.pages.push(x + 1);\r\n        }\r\n    };\r\n    PaginationComponent.prototype.ngOnChanges = function () {\r\n        if (this.pages && this.pages.length) {\r\n            return;\r\n        }\r\n        var numPages = Math.ceil(this.count / 10);\r\n        for (var x = 0; x < numPages; x++) {\r\n            this.pages.push(x + 1);\r\n        }\r\n        console.log('pages', this.pages);\r\n    };\r\n    PaginationComponent.prototype.goToPage = function (page) {\r\n        this.active = page;\r\n        var url = this.url.split('/');\r\n        if (url) {\r\n            url.pop();\r\n            url[url.length - 1] = \"?page=\" + page;\r\n            this.goTo.emit(url.join('/'));\r\n        }\r\n    };\r\n    PaginationComponent.prototype.goUp = function (url) {\r\n        if (this.pages && this.active < this.pages.length && url) {\r\n            this.active++;\r\n            this.goTo.emit(url);\r\n        }\r\n    };\r\n    PaginationComponent.prototype.goDown = function (url) {\r\n        if (this.active > 1 && url) {\r\n            this.active--;\r\n            this.goTo.emit(url);\r\n        }\r\n    };\r\n    tslib_1.__decorate([\r\n        Input(),\r\n        tslib_1.__metadata(\"design:type\", String)\r\n    ], PaginationComponent.prototype, \"next\", void 0);\r\n    tslib_1.__decorate([\r\n        Input(),\r\n        tslib_1.__metadata(\"design:type\", String)\r\n    ], PaginationComponent.prototype, \"previous\", void 0);\r\n    tslib_1.__decorate([\r\n        Input()