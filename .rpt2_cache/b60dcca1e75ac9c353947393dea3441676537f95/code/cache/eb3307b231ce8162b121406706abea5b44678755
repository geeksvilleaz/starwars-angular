{"code":"import * as tslib_1 from \"tslib\";\r\nimport { Component, EventEmitter, Input, Output } from '@angular/core';\r\nimport { Person } from '../models/person';\r\nimport { Planet } from '../models/planet';\r\nimport { Film } from '../models/film';\r\nimport { Species } from '../models/species';\r\nimport { Vehicle } from '../models/vehicle';\r\nimport { Starship } from '../models/starship';\r\nvar ItemComponent = (function () {\r\n    function ItemComponent() {\r\n        this.displayItem = new EventEmitter();\r\n    }\r\n    ItemComponent.prototype.ngOnInit = function () {\r\n        this.loadData();\r\n    };\r\n    ItemComponent.prototype.ngOnChanges = function (changes) {\r\n        this.loadData();\r\n    };\r\n    /**\r\n     * Loads the data onto the page.\r\n     */\r\n    ItemComponent.prototype.loadData = function () {\r\n        this.category = this.getCategoryFromUrl(this.item.url);\r\n        switch (this.category) {\r\n            case 'people':\r\n                this.data = Person.load(this.item);\r\n                break;\r\n            case 'planets':\r\n                this.data = Planet.load(this.item);\r\n                break;\r\n            case 'films':\r\n                this.data = Film.load(this.item);\r\n                break;\r\n            case 'species':\r\n                this.data = Species.load(this.item);\r\n                break;\r\n            case 'vehicles':\r\n                this.data = Vehicle.load(this.item);\r\n                break;\r\n            case 'starships':\r\n                this.data = Starship.load(this.item);\r\n                break;\r\n        }\r\n    };\r\n    /**\r\n     * Callback to display the item detail.\r\n     * @param item The item to be displayed.\r\n     */\r\n    ItemComponent.prototype.handleDisplayItem = function (item) {\r\n        this.displayItem.emit(item);\r\n    };\r\n    /**\r\n     * Gets the category from the API URL.\r\n     * @param url the API URL.\r\n     */\r\n    ItemComponent.prototype.getCategoryFromUrl = function (url) {\r\n        var parts = url.split('/');\r\n        return parts[parts.length - 3];\r\n    };\r\n    tslib_1.__decorate([\r\n        Input(),\r\n        tslib_1.__metadata(\"design:type\", Object)\r\n    ], ItemComponent.prototype, \"item\", void 0);\r\n    tslib_1.__decorate([\r\n        Output(),\r\n        tslib_1.__metadata(\"design:type\", EventEmitter)\r\n    ], ItemComponent.prototype, \"displayItem\", void 0);\r\n    ItemComponent = tslib_1.__decorate([\r\n        Component({\r\n            selector: 'app-item',\r\n            templateUrl: './item.html'\r\n        })\r\n    ], ItemComponent);\r\n    return ItemComponent;\r\n}());\r\nexport { ItemComponent };\r\n//# sourceMappingURL=item.component.js.map","map":{"version":3,"file":"item.component.js","sourceRoot":"","sources":["item.component.ts"],"names":[],"mappings":";AAAA,OAAO,EAAE,SAAS,EAAE,YAAY,EAAE,KAAK,EAAqB,MAAM,EAAE,MAAM,eAAe,CAAC;AAC1F,OAAO,EAAE,MAAM,EAAE,MAAM,kBAAkB,CAAC;AAC1C,OAAO,EAAE,MAAM,EAAE,MAAM,kBAAkB,CAAC;AAC1C,OAAO,EAAE,IAAI,EAAE,MAAM,gBAAgB,CAAC;AACtC,OAAO,EAAE,OAAO,EAAE,MAAM,mBAAmB,CAAC;AAC5C,OAAO,EAAE,OAAO,EAAE,MAAM,mBAAmB,CAAC;AAC5C,OAAO,EAAE,QAAQ,EAAE,MAAM,oBAAoB,CAAC;AAM9C;IAJA;QAMY,gBAAW,GACjB,IAAI,YAAY,EAAyD,CAAC;IA8DhF,CAAC;IAzDC,gCAAQ,GAAR;QACE,IAAI,CAAC,QAAQ,EAAE,CAAC;IAClB,CAAC;IAED,mCAAW,GAAX,UAAY,OAAO;QACjB,IAAI,CAAC,QAAQ,EAAE,CAAC;IAClB,CAAC;IAED;;OAEG;IACH,gCAAQ,GAAR;QACE,IAAI,CAAC,QAAQ,GAAG,IAAI,CAAC,kBAAkB,CAAC,IAAI,CAAC,IAAI,CAAC,GAAG,CAAC,CAAC;QAEvD,MAAM,CAAC,CAAC,IAAI,CAAC,QAAQ,CAAC,CAAC,CAAC;YACtB,KAAK,QAAQ;gBACX,IAAI,CAAC,IAAI,GAAG,MAAM,CAAC,IAAI,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC;gBACnC,KAAK,CAAC;YAER,KAAK,SAAS;gBACZ,IAAI,CAAC,IAAI,GAAG,MAAM,CAAC,IAAI,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC;gBACnC,KAAK,CAAC;YAER,KAAK,OAAO;gBACV,IAAI,CAAC,IAAI,GAAG,IAAI,CAAC,IAAI,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC;gBACjC,KAAK,CAAC;YAER,KAAK,SAAS;gBACZ,IAAI,CAAC,IAAI,GAAG,OAAO,CAAC,IAAI,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC;gBACpC,KAAK,CAAC;YAER,KAAK,UAAU;gBAC